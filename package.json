{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "(index.js)",
  "scripts": {
    "start": "npm build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YasmimVieira/nodejs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YasmimVieira/nodejs/issues"
  },
  "homepage": "https://github.com/YasmimVieira/nodejs#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^15.12.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "module-alias": "^2.2.2",
    "prettier": "^2.8.7",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.3.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
